	keywords = {disability evaluation, epidemiology, health surveys, mental disorders, stereotyping/stigmatization, ANXIETY disorders, SOCIAL stigma, AFFECTIVE disorders -- Social aspects, CHRONIC diseases -- Social aspects, CHRONIC diseases \& psychology, COMPOSITE International Diagnostic Interview, MENTAL health surveys, MENTAL illness -- Social aspects},
	pages = {305--314},
	file = {EBSCO Full Text:C\:\\Users\\abary\\Zotero\\storage\\4E5RBCPE\\Alonso et al. - 2008 - Association of perceived stigma and mood and anxie.pdf:application/pdf}
}

