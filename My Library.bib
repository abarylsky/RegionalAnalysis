	keywords = {epidemiology, Ukraine, sexually transmitted infection},
	pages = {219--221},
	file = {Full Text PDF:C\:\\Users\\abary\\Zotero\\storage\\WFLDC4WA\\Mavrov and Bondarenko - 2002 - The evolution of sexually transmitted infections i.pdf:application/pdf;Snapshot:C\:\\Users\\abary\\Zotero\\storage\\YLATIH3I\\219.html:text/html}
